import java.util.*;
import java.util.ArrayList;
public class listiterarator {
public static void main (String args )
{
	ArrayList<Integer> al= new ArrayList<Integer>();
			al.add(1);
			al.add(7);
			al.add(8);
			
			  Listiterator d = al.Listiterator ();
			  {
				  while(al.hasNext());	
				  {
					  System.out.println(al.Next());
					  }
				  }
}
}
